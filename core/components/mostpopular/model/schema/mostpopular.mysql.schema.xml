<?xml version="1.0" encoding="UTF-8"?>
<model
    package="mostpopular"
    baseClass="xPDOObject"
    platform="mysql"
    defaultEngine="InnoDB"
    phpdoc-package="mostpopular"
    version="1.1">
    <object
        class="MPPageViews"
        table="mp_pageviews"
        extends="xPDOSimpleObject">
        <field
            key="resource"
            dbtype="int"
            precision="10"
            attributes="unsigned"
            phptype="integer"
            null="false"
            index="index" />
        <field
            key="datetime"
            dbtype="timestamp"
            phptype="timestamp"
            default="CURRENT_TIMESTAMP"
            attributes="ON UPDATE CURRENT_TIMESTAMP"
            null="false"
            index="index" />
        <field
            key="data"
            dbtype="varchar"
            precision="16378"
            phptype="json"
            default="[]" />
        <field
            key="ip"
            dbtype="char"
            precision="32"
            phptype="string"
            default="" />

        <index alias="resource" name="resource" primary="false" unique="false" type="BTREE">
            <column key="resource" length="" collation="A" null="false" />
        </index>
        <index alias="datetime" name="datetime" primary="false" unique="false" type="BTREE">
            <column key="datetime" length="" collation="A" null="false" />
        </index>
        <index alias="ip" name="ip" primary="false" unique="false" type="BTREE">
            <column key="ip" length="" collation="A" null="false" />
        </index>
    </object>
</model>
